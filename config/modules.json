{
    "main": {
        "name": "Home",
        "module": ".shop",
        "navigation": [".about"]
    },
    ".about": {
        "name": "About",
        "blueprints": ["about:blueprint"],
        "root": "about",
        "index": "about.about_page",
        "ignore": ["about.bio_page", "about.resume_page", "about.contact_page"]
    },
    ".shop": {
        "name": "Shop",
        "env": [
            "PAYMENT_PROCESSOR",
            "STRIPE_API_KEY"
        ],
        "blueprints": [
            "inventory:blueprint",
            "shop:blueprint",
            "cart_api:blueprint",
            "checkout:blueprint"
        ],
        "root": "shop",
        "index": "shop.index"
    },
    ".blog": {
        "name": "Blog",
        "env": "BLOG_PATH",
        "blueprints": ["blog:blueprint"],
        "root": "blog",
        "index": "blog.blog_index"
    }
}