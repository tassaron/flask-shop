{
    "main": {
        "name": "Home",
        "module": ".about",
        "navigation": [".arcade", ".shop"]
    },
    ".about": {
        "name": "About",
        "blueprints": ["about:blueprint"],
        "root": "about",
        "index": "about.about_page"
    },
    ".arcade": {
        "name": "Arcade",
        "blueprints": [
            "arcade:blueprint"
        ],
        "root": "arcade",
        "index": "arcade.arcade_index",
        "games": {
            "breakout": {
                "title": "Breakout",
                "blurb": "Bounce the ball off your paddle to destroy the bricks. Collect powerups!",
                "style": "width: 640px; height: 480px;",
                "multiplier": 0.025
            },
            "jezzball": {
                "title": "Jezzball",
                "blurb": "Place walls to entrap the balls and flood-fill the level",
                "style": "width: 720px; height: 640px;",
                "multiplier": 0.032,
                "extra_html": "<button style='width: 9rem; height: 3rem;' id='swap_button' type='button'>Swap Direction</button>"
            },
            "speed-limit": {
                "title": "Speed Limit",
                "blurb": "Pass cars while obeying the speed limit! ðŸ›‘",
                "style": "width: 640px; height: 598px;",
                "multiplier": 0.0001
            },
            "rodents-revenge": {
                "title": "Rodent's Revenge",
                "blurb": "Push crates to trap the cats and collect cheese",
                "style": "background:purple; width: 912px; height: 1036px; border: 2px solid black;",
                "multiplier": 0.0002
            }
        }
    },
    ".shop": {
        "name": "Shop",
        "env": [
            "PAYMENT_PROCESSOR"
        ],
        "blueprints": [
            "inventory:blueprint",
            "shop:blueprint",
            "cart_api:blueprint",
            "checkout:blueprint"
        ],
        "root": "shop",
        "index": "shop.index",
        "ignore": ["index"]
    }
}